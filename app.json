{
  "expo": {
    "name": "fun-time",
    "slug": "fun-time",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon2.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.fun.time",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.fun.time",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon2.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
